alScreen = true\r\n      } else {\r\n        this.$store.commit('mutate_isDrawerOpen', false)\r\n        this.isNormalScreen = false\r\n      }\r\n    },\r\n\r\n    Logouter() {\r\n      this.isLogin = false\r\n    }\r\n  },\r\n\r\n\r\n  created(){\r\n    this.statusDrawersSetter()\r\n  },\r\n\r\n\r\n  components: {\r\n    DrawerNormal,\r\n    DrawerSmall,\r\n    Toolbar,\r\n    Header,\r\n    CoreView,\r\n    Footer,\r\n    Login,\r\n    Coco\r\n  },\r\n}\r\n\r\n</script>\r\n\r\n<style>\r\n.v-application {\r\n    font-family: \"Microsoft JhengHei\", \"Roboto\", sans-serif;\r\n    line-height: 1.5;\r\n}\r\n</style>"]}]}