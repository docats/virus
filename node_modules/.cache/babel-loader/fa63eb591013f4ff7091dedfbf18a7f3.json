tion apiPost(url, infoRequest = {}) {\r\n  return new Promise((resolve, reject) => {\r\n    service.post(url, infoRequest).then(\r\n      response => {\r\n        resolve(response.data)\r\n      },\r\n      err => {\r\n        reject(err)\r\n      }\r\n    )\r\n  })\r\n}\r\n\r\nexport function apiGet(url) {\r\n  return new Promise((resolve, reject) => {\r\n    service.get(url).then(\r\n      response => {\r\n        resolve(response)\r\n      },\r\n      err => {\r\n        reject(err)\r\n      }\r\n    )\r\n  })\r\n}"]}]}